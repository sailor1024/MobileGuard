<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="cn.edu.gdmec.android.mobileguard">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGETERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".m1home.HomeActivity"/>
        <activity android:name=".m2theftguard.utils.BaseSetupActivity">
        </activity>
        <activity android:name=".m2theftguard.utils.LostFindActivity">
        </activity>
        <activity android:name=".m2theftguard.utils.Setup1Activity">
        </activity>
        <activity android:name=".m2theftguard.utils.Setup2Activity">
        </activity>
        <activity android:name=".m2theftguard.utils.Setup3Activity">
        </activity>
        <activity android:name=".m2theftguard.utils.Setup4Activity">
        </activity>
    </application>

</manifest>